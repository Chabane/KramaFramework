package org.kramaframework.exception;

/**
 * This exception is generated when developer 
 * added an HTML into HTML Tag without body
 * @author chabane
 * @version 1.0
 */
public class KramaIllegalAddTagException extends Exception {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * Constructor
	 * @param message
	 */
	public KramaIllegalAddTagException(String message) {
		super(message);
	}
}
