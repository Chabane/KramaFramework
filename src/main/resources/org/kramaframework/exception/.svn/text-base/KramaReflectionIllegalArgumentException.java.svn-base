package org.kramaframework.exception;

/**
 * This exception is generated when developer used a wrong information 
 * when put a parameter in javascript function
 * @author chabane
 * @version 1.0
 */
public class KramaReflectionIllegalArgumentException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * Constructor
	 * @param message
	 * @param exception
	 */
	public KramaReflectionIllegalArgumentException(String message, IllegalArgumentException exception) {
		super(message, exception);
	}

	/**
	 * Constructor
	 * @param message
	 */
	public KramaReflectionIllegalArgumentException(String message) {
		super(message);
	}
}
